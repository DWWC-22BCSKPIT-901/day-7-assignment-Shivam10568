#include <vector>
using namespace std;

int findJudge(int n, vector<vector<int>>& trust) {
    vector<int> trustScores(n + 1, 0);

    for (auto& t : trust) {
        trustScores[t[0]]--;
        trustScores[t[1]]++;
    }

    for (int i = 1; i <= n; ++i) {
        if (trustScores[i] == n - 1) return i;
    }
    return -1;
}

int main() {
    vector<vector<int>> trust = {{1, 3}, {2, 3}};
    cout << findJudge(3, trust); // Output: 3
}
